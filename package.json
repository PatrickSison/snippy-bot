    "dotenv": "^16.0.3",
    "ffmpeg-static": "^5.1.0",
    "lodash": "^4.17.21",
    "openai": "^3.2.1",
    "yt-search": "^2.10.4",
    "ytdl-core": "^4.11.3"
  },
  "devDependencies": {
    "nodemon": "^2.0.22",
